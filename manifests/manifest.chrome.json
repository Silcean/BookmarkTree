{
  "name": "Bookmark Tree",
  "description": "Adds a grafical gui to bookmarks in the newtab window",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["bookmarks", "activeTab", "favicon", "storage"],
  "chrome_url_overrides": {
    "newtab": "/newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "/css/iconWhite16.png",
    "32": "/css/iconWhite32.png",
    "48": "/css/iconWhite48.png",
    "128": "/css/iconWhite128.png"
  }
}
