{
  "name": "Visual Bookmark Tree manifest",
  "description": "Adds a graphical GUI to bookmarks in the new tab window",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://www.google.com/s2/*",
    "https://t0.gstatic.com/faviconV2*",
    "https://t1.gstatic.com/faviconV2*",
    "https://t2.gstatic.com/faviconV2*",
    "https://t3.gstatic.com/faviconV2*",
    "https://t4.gstatic.com/faviconV2*"
  ],
  "chrome_url_overrides": {
    "newtab": "/newtab.html"
  },
  "chrome_settings_overrides": {
    "homepage": "/newtab.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "/icons/iconWhite16.png",
    "32": "/icons/iconWhite32.png",
    "48": "/icons/iconWhite48.png",
    "128": "/icons/iconWhite128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "@bookmark-tree",
      "strict_min_version": "58.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}
