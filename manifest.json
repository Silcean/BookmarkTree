{
  "name": "Visual Bookmark Tree manifest",
  "description": "Adds a graphical GUI to bookmarks in the new tab window",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "activeTab",
    "storage"
  ],
    "chrome_url_overrides": {
        "newtab": "/newtab.html"
    },
    "chrome_settings_overrides": {
        "homepage": "/newtab.html"
    },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}